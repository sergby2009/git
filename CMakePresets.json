{
    "version": 8,
    "configurePresets": [
        {
            "name": "test",
            "displayName": "GCC 14.2.0 i686-w64-mingw32",
            "description": "Использование компиляторов: C = D:\\mingw32\\bin\\gcc.exe, CXX = D:\\mingw32\\bin\\g++.exe",
            "generator": "MinGW Makefiles",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "D:/mingw32/bin/gcc.exe",
                "CMAKE_CXX_COMPILER": "D:/mingw32/bin/g++.exe",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}